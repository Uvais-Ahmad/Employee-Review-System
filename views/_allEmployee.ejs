<link href="/css/adminBackground.css" rel="stylesheet" type="text/css">

<table class="table table-striped table-dark w-75 mx-auto mt-5" style="margin-bottom:44vh">
    
    <thead>
        <tr>
            <th scope="col">Sr No.</th>
            <th scope="col">Name</th>
            <th scope="col">Email</th>
            <th scope="col">Admin</th>
            <th scope="col" class="w-25">Remove Employee</th>
        </tr>
    </thead>

    <tbody>
        <% let i=1; %> 
        <% for(user of users){ %>

            <tr>
                <th scope="row"><%= i %> </th> 
                <td><%= user.name %> </td>
                <td><%= user.email %> </td>
                <td><%= user.isAdmin %> </td>
                <td class="w-25"><a role="button" href="#" class="btn btn-danger">Delete</a></td>
            </tr>
            <% i++; %> 
        <% } %> 
        
    </tbody>
  </table>
